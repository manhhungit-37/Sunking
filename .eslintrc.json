{
    "extends": "next/core-web-vitals",
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "react/react-in-jsx-scope": "off"
    },
    "parserOptions": {
        "ecmaVersion": 2020
    }
}
